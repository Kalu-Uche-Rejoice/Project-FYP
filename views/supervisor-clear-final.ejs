<body>
  <!-- Layout wrapper -->
  <div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
      <!-- Menu -->

     
      <!-- / Menu -->

      <!-- Layout container -->
      <div class="layout-page">
        <!-- Navbar -->

        <nav class="layout-navbar container-xxl navbar navbar-expand-xl navbar-detached align-items-center bg-navbar-theme" id="layout-navbar">
          <div class="layout-menu-toggle navbar-nav align-items-xl-center me-3 me-xl-0 d-xl-none">
            <a class="nav-item nav-link px-0 me-xl-4" href="javascript:void(0)">
              <i class="bx bx-menu bx-sm"></i>
            </a>
          </div>

          <div class="navbar-nav-right d-flex align-items-center" id="navbar-collapse">
            <!-- Search -->
            <div class="navbar-nav align-items-center">
              <div class="nav-item d-flex align-items-center">
              </div>
            </div>
            <%- include('partials/notification.ejs') %>
            <!-- /Search -->
          
        

        <!-- / Navbar -->

        <!-- Content wrapper -->
        
          <div class="content-wrapper">
            <!-- Content -->
  
            <div class="container-xxl flex-grow-1 container-p-y">
              <h4 class="fw-bold py-3 mb-4"><a href="/users/supervisor/clear-final"></a>
                Clearance  Module</h4>
              <div class="row">
                <div class="col">
                <!-- Form controls -->
                              
                <% for( let i = 0; i < proposal.length; i++ ) { %>
                  
                  <div class="card mb-4">
                    <div class="card-header">
                      
                      <p> <b>Name: </b>
                        <p id="name<%=i%>"><%= proposal[i].author %></p>
                      </p>
                      <p> <b>Course: </b>
                        <p><%= proposal[i].course %></p>
                      </p>
                    </div>
                    <div class="card-body py-0">
                     <div class="row">
                      <div class="col">
                        Project Title:   
                        <p id="topic<%=i%>"><%= proposal[i].topic %></p>
                        <button class="btn btn-outline-secondary" type="submit" id="button">
                          <a href="<%= proposal[i].downloadURL %>" target="_blank">Download Final Submission</a>
                        </button>
                        <div class="input-group-text my-3">
                          <button class="btn btn-success" type="submit" id="button" onclick="test('<%=i%>', true)">Accept</button>
                          <button class="btn btn-danger mx-4" type="submit" id="button" onclick="test('<%=i%>', false)">Reject</button>
                        </div>
                      </div>
                     </div>
                    </div>
                  </div>
                
              <% } %>
                </div>
              </div>
            </div>
            <!-- / Content -->
  
            <div class="content-backdrop fade"></div>
          </div>
          <!-- / Content -->

          
        
        <!-- Content wrapper -->
      </div>
      <!-- / Layout page -->
    </div>

    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>
  
  <!-- / Layout wrapper -->
  <!-- Core JS -->
  <!-- build:js assets/vendor/js/core.js -->
