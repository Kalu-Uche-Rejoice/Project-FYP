          </div>
        </nav>

        <!-- / Navbar -->

        <!-- Content wrapper -->
        <div class="content-wrapper">
          <div class="container-xxl flex-grow-1 container-p-y">
            <h4 class="fw-bold py-1 mb-4">Project Monitoring Module</h4>
            <div class="progress my-3">
              <div class="progress-bar" role="progressbar" style="width: 75%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">
                75%
              </div>
            </div>
            <div class="row">
              <div class="col-md-4 card">
                <div class="card-body">
                  <label for="supervisorComment" class="form-label"><h5>Supervisor's Last week comments</h5></label>
                  <textarea class="form-control" id="supervisorComment" rows="10" readonly=""></textarea>
                 </div>
                <div class="card-footer">
                  <button class="btn btn-primary btn-lg px-5" id="button" onclick="toggleModal()">View Previous Logs</button> 
                </div>
              </div>
              <div class="col-md-8">
                <div class="card mb-4">
                  <div class="card-body">
                    <form action="/users/student/log" method="post" enctype="multipart/form-data">
                      <div>
                        <label for="WeekNo" class="form-label"><strong>WEEK NUMBER</strong></label>
                        <select id="WeekNo" class="form-select form-select-sm" name="week">
                          <option value="1">One</option>
                          <option value="2">Two</option>
                          <option value="3">Three</option>
                          <option value="4">four</option>
                          <option value="5">five</option>
                          <option value="6">six</option>
                          <option value="7">seven</option>
                          <option value="8">eigt</option>
                          <option value="9">nine</option>
                          <option value="10">ten</option>
                          <option value="11">eleven</option>
                          <option value="12">twelve</option>
                          <option value="13">thirteen</option>
                          <option value="14">Fourteen</option>
                          <option value="15">Fifteen</option>
                          <option value="16">Sixteen</option>
                          <option value="17">Seventeen</option>
                          <option value="18">Eighteen</option>
                          <option value="19">nineteen</option>
                          <option value="20">Twenty</option>
                        </select>
                      </div>
                      <div class="my-3">
                        <label for="LogView" class="form-label"><h5>Project Progress Log</h5></label>
                        <textarea class="form-control" id="LogView" rows="4" required="" name="log"></textarea>
                      </div>
                      <div class="row mb-3">
                        <div class="col">
                          <label for="formFile" class="form-label">Upload file</label>
                          <input class="form-control" type="file" id="formFile" accept=".pdf, .doc, .docx, application/msword" multiple="" name="logfile">
                         </div>
                        <div class="col">
                          <label for="html5-datetime-local-input" class="form-label">Request a meeting with your supervisor</label>
                         <input class="form-control" type="datetime-local"  id="html5-datetime-local-input" name="meeting">
                        </div>
                      </div>
                      <input type="submit" value="submit"class="btn btn-primary btn-lg px-5">
                    </form>
                  </div>
                  
                </div>
              </div>
              <div class="col-md mb-4 mb-md-0" style="display: none;" id="pastlog">
                <small class="text-light fw-semibold">Past Logs</small>
                <div class="accordion mt-3" id="accordionExample">
                  <% log.forEach(element => { %>
                    <div class="card accordion-item active">
                      <h2 class="accordion-header" id="headingOne">
                        <button
                          type="button"
                          class="accordion-button"
                          data-bs-toggle="collapse"
                          data-bs-target="#<%=element.week%>"
                          aria-expanded="true"
                          aria-controls="<%=element.week%>"
                        >
                        Week <%=element.week%>
                        </button>
                      </h2>
  
                      <div
                        id="<%=element.week%>"
                        class="accordion-collapse collapse show"
                        data-bs-parent="#accordionExample"
                      >
                        <div class="accordion-body">
                          <%=element.log%>
                        </div>
                      </div>
                    </div>
                  <% }) %>
                </div>
              </div>
            </div>
          </div>
        </div>
