<script>
  function test(id, num, status) {
    var nameID = "fullname" + id
    var topicID = "topic" + id + num
    console.log(topicID)
    var name = document.getElementById(nameID).innerHTML
    var topic = document.getElementById(topicID).innerHTML
    console.log(topic)
    var split = name.split("</b>")
    $.post("/users/supervisor/proposals",
        {
            name: `${split[1]}`,
            topic: `${topic}`,
            status: `${status}`
        },)
    var rem = document.getElementById(name)
    rem.style.display="none"
  }
</script>
